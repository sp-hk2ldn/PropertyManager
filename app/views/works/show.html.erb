<div class="page-header">
  <%= link_to works_path, :class => 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_work_path(@work), :class => 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show work</h1>
</div>

<dl class="dl-horizontal">
  <dt>Type:</dt>
  <dd><%= @work.type %></dd>

  <dt>Raised date:</dt>
  <dd><%= @work.raised_date %></dd>

  <dt>Due date:</dt>
  <dd><%= @work.due_date %></dd>

  <dt>Property:</dt>
  <dd><%= @work.property_id %></dd>

</dl>
