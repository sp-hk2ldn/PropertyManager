<div class="page-header">
  <%= link_to contractors_path, :class => 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_contractor_path(@contractor), :class => 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show contractor</h1>
</div>

<dl class="dl-horizontal">
  <dt>Company name:</dt>
  <dd><%= @contractor.company_name %></dd>

  <dt>Industry type:</dt>
  <dd><%= @contractor.industry_type %></dd>

  <dt>Email:</dt>
  <dd><%= @contractor.email %></dd>

  <dt>Tel:</dt>
  <dd><%= @contractor.tel %></dd>

  <dt>Jobs completed:</dt>
  <dd><%= @contractor.jobs_completed %></dd>

  <dt>Jobs feedback good:</dt>
  <dd><%= @contractor.jobs_feedback_good %></dd>

  <dt>Jobs feedback bad:</dt>
  <dd><%= @contractor.jobs_feedback_bad %></dd>

</dl>
