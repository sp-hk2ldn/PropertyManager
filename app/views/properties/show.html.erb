<div class="page-header">
  <%= link_to properties_path, :class => 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_property_path(@property), :class => 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show property</h1>
</div>

<dl class="dl-horizontal">
  <dt>Address:</dt>
  <dd><%= @property.address %></dd>

  <dt>Landlord:</dt>
  <dd><%= @property.landlord_id %></dd>

  <dt>Longitude:</dt>
  <dd><%= @property.longitude %></dd>

  <dt>Latitude:</dt>
  <dd><%= @property.latitude %></dd>

  <dt>Works:</dt>
  <dd><%= @property.works %></dd>

  <dt>Work type:</dt>
  <dd><%= @property.work_type %></dd>

</dl>
